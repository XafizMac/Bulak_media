{
  "expo": {
    "name": "Bulak Media",
    "slug": "bulakmedia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/backgrouds/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#2E0A30"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hafizbeyyyy.tasbihat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#2E0A30"
      },
      "package": "com.hafizbeyyyy.tasbihat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4a52fd8d-fd2c-4fcb-af48-b90577e9eaa6"
      }
    },
    "owner": "hafizbeyyyy",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}


